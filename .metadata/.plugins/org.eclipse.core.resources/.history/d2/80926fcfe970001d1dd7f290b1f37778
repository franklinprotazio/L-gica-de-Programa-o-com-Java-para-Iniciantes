package encapsulamento;

public class Cliente {

//	String nome;
	
	String primeiroNome;
	
	String ultimoNome;
	
	String telefone;
	
	String getNome() {
		return primeiroNome + " " + ultimoNome;
	}
	
	void setNome(String nome) {
		this.nome = nome;
	}
	
	String getTelefone() {
		return telefone;
	}
	
	void setTelefone(String telefone) {
		this.telefone = telefone;
	}
}
