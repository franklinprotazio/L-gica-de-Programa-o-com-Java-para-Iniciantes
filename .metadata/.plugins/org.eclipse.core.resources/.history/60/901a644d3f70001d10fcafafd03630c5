package exercicio09;

public class Exercicio09 {

	public static void main(String[] args) {
		Produto produto = new Produto();
		produto.nome = "Camisa";
		produto.quantidadeEstoque = 10;
		
		validarQuantidadeMinima(0);
	}
	
	static Boolean validarQuantidadeMinima(Integer quantidadeMinima) {
		Boolean valida = quantidadeMinima >= 10;
		System.out.println(valida);
		return valida;
	}

}
