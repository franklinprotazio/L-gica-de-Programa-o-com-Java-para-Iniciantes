package exercicio09;

public class Exercicio09 {

	public static void main(String[] args) {
		Produto produto = new Produto();
		produto.nome = "Camisa";
		produto.quantidadeEstoque = 10;
		
		validarQuantidadeMinima(produto.quantidadeEstoque);
	}
	
	static Boolean validarQuantidadeMinima(Integer quantidadeMinima) {
		Boolean valida = quantidadeMinima < 10;
		
		if(valida) {
			System.out.println("Eh necessario a reposicao");
		} else{
			System.out.println("Teste");
		}
		return valida;
	}

}
